<h2> New Trip </h2>
    <%= form_for [:admin, @trip] do |f| %>
    <%= f.label :duration %>
    <%= f.text_field :duration, placeholder: "new category*" %><br>
    <%= f.label :start_date %>
    <%= f.text_field :start_date, placeholder: "new category*" %><br>
    <%= f.label :start_station_id %>
    <%= f.text_field :start_station_id, placeholder: "new category*" %><br>
    <%= f.label :end_date %>
    <%= f.text_field :end_date, placeholder: "new category*" %><br>
    <%= f.label :end_station_id %>
    <%= f.text_field :end_station_id, placeholder: "new category*" %><br>
    <%= f.label :bike_id %>
    <%= f.text_field :bike_id, placeholder: "new category*" %><br>
    <%= f.label :subscription %>
    <%= f.text_field :subscription, placeholder: "new category*" %><br>
    <%= f.label :zipcode %>
    <%= f.text_field :zipcode, placeholder: "new category*" %><br>
    <%= f.submit class: "sub-btn" %>
    <% end %>
