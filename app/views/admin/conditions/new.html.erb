    <%= f.text_field :mean_dewpoint_f, placeholder: "*" %>
  </div>
  <div class="form-field">
    <%= f.label :min_dewpoint_f %>
    <%= f.text_field :min_dewpoint_f, placeholder: "*" %>
  </div>
  <div class="form-field">
    <%= f.label :max_humidity %>
    <%= f.text_field :max_humidity, placeholder: "*" %>
  </div>
  <div class="form-field">
    <%= f.label :mean_humidity %>
    <%= f.text_field :mean_humidity, placeholder: "*" %>
  </div>
  <div class="form-field">
    <%= f.label :min_humidity %>
    <%= f.text_field :min_humidity, placeholder: "*" %>
  </div>
  <div class="form-field">
    <%= f.label :max_sea_level_pressure_inches %>
    <%= f.text_field :max_sea_level_pressure_inches, placeholder: "*" %>
  </div>
  <div class="form-field">
    <%= f.label :mean_sea_level_pressure_inches %>
