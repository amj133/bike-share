    <%= f.label :min_visibility_miles %>
    <%= f.text_field :min_visibility_miles, placeholder: "*" %>
  </div>
  <div class="form-field">
    <%= f.label :max_wind_speed %>
    <%= f.text_field :max_wind_speed, placeholder: "*" %>
  </div>
  <div class="form-field">
    <%= f.label :mean_wind_speed %>
    <%= f.text_field :mean_wind_speed, placeholder: "*" %>
  </div>
  <div class="form-field">
    <%= f.label :events %>
    <%= f.text_field :events, placeholder: "*" %>
