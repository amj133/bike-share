<h2> New Station </h2>
    <%= form_for [:admin, @station] do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, placeholder: "new category*" %><br>
    <%= f.label :lat %>
    <%= f.text_field :lat, placeholder: "new category*" %><br>
    <%= f.label :long %>
    <%= f.text_field :long, placeholder: "new category*" %><br>
    <%= f.label :dock_count %>
    <%= f.text_field :dock_count, placeholder: "new category*" %><br>
    <%= f.label :city %>
    <%= f.text_field :city, placeholder: "new category*" %><br>
    <%= f.label :installation_date %>
    <%= f.text_field :installation_date, placeholder: "new category*" %><br>
    <%= f.hidden_field :slug %>
    <%= f.submit class: "sub-btn" %>
    <% end %>
